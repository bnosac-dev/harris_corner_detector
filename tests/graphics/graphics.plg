Yt=1.1


#SUBPIXEL GRAPHICS
set terminal postscript enhanced dashed
set output "subpixel_quadratic.eps"
set xlabel "{rotation angle in degrees}"
set xrange [0:180]
set ylabel "repeatability rate"
set yrange [0:Yt]
set ytics 0,0.1,Yt
set key bottom
plot "../subpixel_quadratic.txt" using ($1*180/pi):2 title "{/Symbol e}=0.5" with lines, "../subpixel_quadratic.txt" using ($1*180/pi):4 title "{/Symbol e}=1.5" with lines, "../subpixel_quadratic.txt" using ($1*180/pi):7 title "{/Symbol e}=3.0" with lines, "../subpixel_quadratic.txt" using ($1*180/pi):11 title "{/Symbol e}=5.0" with lines

